<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SolicitudService"
                   namespace="SolicitudService.Dominio"
                   default-lazy="false">
  <class name="SolicitudDetalle" table="t_solicitud_detalle">
    <id name="Id" column="id">
      <generator class="increment" />
    </id>
    <property name="Codigo" column="codigo" />
    <property name="CantidadColabor" column="cantidadColabor" />
    <property name="Perfil" column="codPerfil" />
    <property name="Tecnologia" column="codTecnologia" />
    <property name="ExperienciaRubro" column="codExperienciaRubro" />
    <property name="Comentario" column="comentario" />
  </class>
</hibernate-mapping>
